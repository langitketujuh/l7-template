do_install() {
	vinstall ${FILESDIR}/profiles.ini 0644 etc/skel/.config/falkon/profiles profiles.ini
	vinstall ${FILESDIR}/settings.ini 0644 etc/skel/.config/falkon/profiles/default settings.ini
}
