# Template file for 'l7-removed-packages'
pkgname=l7-removed-packages
version=0.1
revision=1
build_style=meta
short_desc="Uninstalls packages removed from LangitKetujuh repository"
maintainer="Hervy Qurrotul Ainur Rozi <hervyqa@pm.me>"
license="Public Domain"
homepage="https://langitketujuh.id"

replaces="
  packages<=0.0.0.0_1
"

if [ "$XBPS_TARGET_LIBC" = "musl" ]; then
  replaces+="
    packages<=0.0.0.0_1
    "
fi

case "$XBPS_TARGET_MACHINE" in
  ppc64*) ;;
  ppc*)
    replaces+="
      packages<=0.0.0.0_1
    "
    ;;
esac
