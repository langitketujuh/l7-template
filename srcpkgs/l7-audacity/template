do_install() {
	vinstall ${FILESDIR}/audacity.cfg 0644 etc/skel/.audacity-data audacity.cfg
	vinstall ${FILESDIR}/ImageCache.png 0644 etc/skel/.audacity-data/Theme ImageCache.png
}
