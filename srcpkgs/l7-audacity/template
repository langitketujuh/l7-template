# Template file for 'l7-audacity'
pkgname=l7-audacity
version=2.4.1
revision=2
depends="audacity"
short_desc="LangitKetujuh patch for audacity package"
maintainer="Hervy Qurrotul Ainur Rozi <hervyqa@pm.me>"
